{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2019
    },
    "plugins": ["async-await", "babel"],
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-async-promise-executor": 0,
        "no-prototype-builtins": 0,
        "async-await/space-after-async": 2,
        "async-await/space-after-await": 2
    },
    "globals": {
        "URLSearchParams": false,
        "ga": false
    }
}
