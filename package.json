{
  "name": "std-js",
  "version": "1.0.0",
  "description": "A JavaScript library for making front-end development sane!",
  "keywords": [
    "ECMAScript2015",
    "ES6",
    "JavaScript",
    "modules",
    "front-end",
    "polyfill",
    "shim"
  ],
  "author": {
    "name": "Chris Zuber",
    "email": "shgysk8zer0@gmail.com",
    "url": "https://chriszuber.com"
  },
  "contributors": [],
  "homepage": "https://chriszuber.com/",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/shgysk8zer0/std-js"
  },
  "bugs": {
    "url": "https://github.com/shgysk8zer0/std-js/issues"
  },
  "engines": {
    "npm": ">=3.5"
  },
  "scripts": {
    "build:css": "postcss test/index.css -o test/index.min.css -u postcss-import postcss-cssnext cssnano -m",
    "build:js": "webpack",
    "build:all": "npm run build:css && npm run build:js",
    "lint:js": "eslint scripts",
    "watch:js": "webpack -w",
    "watch:css": "postcss test/index.css -o test/index.min.css -u postcss-import postcss-url postcss-cssnext cssnano -m -w",
    "git:fetch": "git fetch --all --prune --tags",
    "update-remote": "npm update --save",
    "test": "eslint .",
    "start": "python -m SimpleHTTPServer 8000",
    "postinstall": "npm run git:fetch && npm run build:all"
},
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "^4.0.1"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "cssnano": "^3.10.0",
    "postcss": "^6.0.1",
    "postcss-cli": "^4.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-url": "^7.3.0",
    "webpack": "^3.10.0"
  }
}
